<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.frame.business.entity">
	<class
		name="EmployeePositionFulfillment"
		table="employee_position_fulfillment"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="Id" class="EmployeePositionFulfillmentPK">
			<key-many-to-one
				name="Employee"
				class="Employee"
				column="employee_id"
			/>
			<key-many-to-one
				name="PartyGroupPosition"
				class="PartyGroupPosition"
				column="party_group_position_id"
			/>
		</composite-id>

		<property
			name="EmploymentType"
			column="employment_type"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Location"
			column="location"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="CurrentRole"
			column="current_role"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="FromDate"
			column="from_date"
			type="date"
			not-null="true"
			length="10"
		/>
		<property
			name="ThruDate"
			column="thru_date"
			type="date"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>