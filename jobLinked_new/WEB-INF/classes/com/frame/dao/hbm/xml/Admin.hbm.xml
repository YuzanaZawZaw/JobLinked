<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.frame.business.entity">
	<class
		name="Admin"
		table="admin"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="admin_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Email"
			column="email"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="Password"
			column="password"
			type="string"
			not-null="false"
			length="50"
		/>


	</class>	
</hibernate-mapping>