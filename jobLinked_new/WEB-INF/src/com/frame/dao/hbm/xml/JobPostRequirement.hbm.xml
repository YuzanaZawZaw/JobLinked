<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.frame.business.entity">
	<class
		name="JobPostRequirement"
		table="job_post_requirement"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="job_post_requirement_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Description"
			column="description"
			type="string"
			not-null="false"
			length="225"
		/>
		<many-to-one
			name="JobPost"
			column="job_post_id"
			class="JobPost"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>